
#구입 금액 입력
purchased_money = input("구입금액을 입력해 주세요.")
print (purchased_money)

#구입 총 장수 출력  
ea = int(purchased_money)//1000
print( ea,"장의 로또를 구입하셨습니다.")

#랜덤 생성 로또 출력 
purchased_ea =0
while purchased_ea < ea :
    import random
    purchased_ea+=1
    lotto_numbers = random.sample(range(1, 45), 6)
    lotto_numbers.sort()
    print(lotto_numbers)

#지난 주 당첨 번호 입력

last_win_num =list( input("지난주 당첨 번호를 입력해주세요. : ").split(','))
last_win_num.sort()


#로또 당첨 결과 출력
print ("로또당첨결과 : ")

score = 0
for k in range(0,len(lotto_numbers)):
    for v in range(0, len(last_win_num)):
        if lotto_numbers[k] == last_win_num[v]:
            score += 1
            
if score == 6:
    print("1등-500000원")
    
elif score == 5:
    print("2등-100000원")
    
elif score == 4:
    print("3등-20000원")
      
elif score == 3:
    print("4등-5000원 ")

else : print("미당첨")



